        <th scope="col">Etkinlik AdÄ±</th>
        <th scope="col">Etkinlik Tarihi</th>
        <th scope="col">Etkinlik Yeri</th>
    </tr>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td> <img style="max-height: 120px; align-items: center;" src="@Html.DisplayFor(modelItem => item.ImageUrl)"></td>
